class IntIterator {
    var value
    var start
    var end

    var step = 1

    func __init(start, end){
        value = start
        value = end
    }

    func hasNext {
        return value < end
    }

    func next {
        value += step
        return value - step
    }
}

class IntRange {
    func __iter {
        return IntIterator();
    }
}